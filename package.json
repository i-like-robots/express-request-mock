{
  "name": "express-request-mock",
  "version": "3.0.0",
  "description": "Makes testing Express route callbacks and controllers easy.",
  "keywords": [
    "mock",
    "stub",
    "test",
    "express",
    "controller"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "npm run format && npm run spec",
    "format": "prettier --config .prettierrc.json --list-different **/*.js",
    "spec": "tap test/spec.js",
    "coverage": "npm run spec -- --coverage"
  },
  "author": "Matt Hinchliffe",
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/express-request-mock.git"
  },
  "homepage": "https://github.com/i-like-robots/express-request-mock",
  "license": "MIT",
  "dependencies": {
    "node-mocks-http": "^1.9.0"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "prettier": "^2.2.1",
    "sinon": "^10.0.0",
    "tap": "^15.0.0"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
