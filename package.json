{
  "name": "express-request-mock",
  "version": "2.0.0",
  "description": "A convenient wrapper for node-mocks-http to make testing Express route callbacks and controllers easy.",
  "keywords": [
    "mock",
    "stub",
    "test",
    "express",
    "controller"
  ],
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:spec",
    "test:lint": "standard",
    "test:spec": "tap test/spec.js",
    "test:coverage": "npm run test:spec -- --coverage"
  },
  "author": "Matt Hinchliffe",
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/express-request-mock.git"
  },
  "homepage": "https://github.com/i-like-robots/express-request-mock",
  "license": "MIT",
  "dependencies": {
    "node-mocks-http": "^1.6.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "sinon": "^5.0.0",
    "standard": "^11.0.0",
    "tap": "^11.1.5"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
