    "url": "git@github.com:i-like-robots/express-request-mock.git"
  },
  "homepage": "https://github.com/i-like-robots/express-request-mock",
  "license": "MIT",
  "dependencies": {
    "node-mocks-http": "^1.7.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "sinon": "^9.0.0",
    "standard": "^14.0.0",
    "tap": "^14.0.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
